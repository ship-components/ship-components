  "description": "React checkbox component",
  "main": "src/index.js",
  "scripts": {
    "start": "grunt server",
    "test": "jest src/ --coverage",
    "lint": "eslint src/*.jsx",
    "coverage": "cat ./coverage/lcov.info | coveralls -v"
  },
  "repository": {
    "type": "git",
